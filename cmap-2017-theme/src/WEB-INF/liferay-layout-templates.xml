<?xml version="1.0"?>
<!DOCTYPE layout-templates PUBLIC "-//Liferay//DTD Layout Templates 7.0.0//EN" "http://www.liferay.com/dtd/liferay-layout-templates_7_0_0.dtd">

<layout-templates>
	<custom>
		<layout-template id="cmap" name="CMAP Layout">
			<template-path>/layouttpl/custom/cmap-layouttpl/cmap.tpl</template-path>
			<thumbnail-path>/layouttpl/custom/cmap-layouttpl/cmap.png</thumbnail-path>
		</layout-template>
		<layout-template id="cmap-finished" name="CMAP Finished Layout">
			<template-path>/layouttpl/custom/cmap-finished-layouttpl/cmap-finished.tpl</template-path>
			<thumbnail-path>/layouttpl/custom/cmap-finished-layouttpl/cmap-finished.png</thumbnail-path>
		</layout-template>
		<layout-template id="cmap-nested" name="CMAP Nested Layout">
			<template-path>/layouttpl/custom/cmap-nested-layouttpl/cmap-nested.tpl</template-path>
			<thumbnail-path>/layouttpl/custom/cmap-nested-layouttpl/cmap-nested.png</thumbnail-path>
		</layout-template>
	</custom>
</layout-templates>
