<!DOCTYPE html>

#parse ($init)

#set($userAgent = $request.getHeader("User-Agent").toLowerCase())
#set($is_bot = false)
#if($userAgent.matches(".*(bot|Bot|xenu|Xenu|spider|Spider|aggregator|Aggregator|Thirdwave|thirdwave).*"))
  #set($is_bot = true)
#end

<html class="#language("lang.dir")" dir="#language("lang.dir")" lang="$w3c_language_id">

<head>
	<title>$the_title - $company_name</title>

  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />

  <meta name="google-site-verification" content="iMsEAZsT6-h_U3rB41bbxpCB49JA_ycS1A10eU6Ligc" />
  <meta name="google-site-verification" content="vT3ubvzyQ8I_4d3q4K-veEExjAoZb6AeiJQdhJbFyDw" />

  <link rel="shortcut icon" type="image/png" href="$images_folder/favicon.ico" />

  <script type="text/javascript" src="//use.typekit.net/mfi5lqs.js"></script>
  <script type="text/javascript">try{Typekit.load();}catch(e){}</script>
  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js" type="text/javascript"></script>
  <script>
    if (!window.jQuery) {
      document.write('<script src="$javascript_folder/jquery.js" type="text/javascript"><\/script>');
    }
  </script>
  <script src="//ajax.googleapis.com/ajax/libs/jqueryui/1/jquery-ui.min.js" type="text/javascript"></script>
  <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/modernizr/2.7.1/modernizr.min.js"></script>
  <!--[if lt IE 9]>
    <script src="$javascript_folder/html5shiv.js" type="text/javascript"></script>
  <![endif]-->

  
  #set($userGroupRoleLocalService = $serviceLocator.findService("com.liferay.portal.service.UserGroupRoleLocalService"))
  #set ($hasRole = $userGroupRoleLocalService.hasUserGroupRole($user_id, $group_id, "Content Editor"))

  #if($hasRole)
    <script src="$javascript_folder/jquery.multiselect.min.js" type="text/javascript"></script>
    <script src="$javascript_folder/jquery.multiselect.filter.js" type="text/javascript"></script>
    <link rel="stylesheet" type="text/css" href="http://ajax.googleapis.com/ajax/libs/jqueryui/1/themes/ui-lightness/jquery-ui.css" />
    <link href="$css_folder/jquery.multiselect.css" type="text/css" rel="stylesheet" />
    <link href="$css_folder/jquery.multiselect.filter.css" type="text/css" rel="stylesheet" />
  #end

	$theme.include($top_head_include)
</head>

#set($layouttpl = $layout.getTypeSettingsProperties().getProperty("layout-template-id") + "-container")

<body class="cmapMobilityApp $css_class $layouttpl">

$theme.include($body_top_include)

#if ($is_signed_in)
	#dockbar()
#end

<div id="page">
	<a href="#main-content" id="skip-to-content" class="hidden">#language("skip-to-content")</a>

  <div class="type-ahead-search container-fluid">
    <div class="row-fluid">
      <h2>Search</h2>
      #if (!$is_bot)
        #set ($myPortletId = "lookaheadsearch_WAR_CustomPortletsportlet")
        $theme.runtime($myPortletId)
      #end
    </div>
  </div>

	<header id="banner" role="banner" class="site-header">
		<div class="header-main">
      <div class="container-fluid">
        <div class="row-fluid">

          <div class="span2 branding">
            <h1 class="site-title">
              <a href="$site_default_url" class="site-name" title="#language("go-to") $site_name">
                <span>$site_name</span>
                <img alt="$logo_description" src="$site_logo" class="pull-right logo" />
              </a>
            </h1>
          </div> <!-- .branding -->

          <nav class="span6 pull-right nav-meta">
            <ul class="items items-inline">
              <li class="events"><a href="/events" title="CMAP Events">Events</a></li>
              <li class="search"><a href="#search" title="Search cmap.illinois.gov" class="icon">Search</a></li>
            </ul>
          </nav> <!-- .nav-meta -->

        </div> <!-- .row-fluid -->
      </div> <!-- .container-fluid -->
    </div> <!-- .header-main -->

		<div class="site-navigation">
      #if ($has_navigation)
        #parse ("$full_templates_path/navigation.vm")
      #end
    </div> <!-- .site-navigation -->

	</header>

	<div id="content" class="site-main">
		#if ($selectable)
			$theme.include($content_include)
		#else
			$portletDisplay.recycle()

			$portletDisplay.setTitle($the_title)

			$theme.wrapPortlet("portlet.vm", $content_include)
		#end
	</div>

	<footer id="footer" role="contentinfo" class="site-footer">
    <div class="container-fluid">
      <div class="row-fluid">
        <div class="span9 pull-right footer-1">
          #if ( $footer_section_1 )
            $footer_section_1
          #end
        </div> <!-- .footer-1 -->
      </div> <!-- .row-fluid -->
    </div> <!-- .container-fluid -->
	</footer>


</div>

<div id="loadingStatus" class="loading-status">
  <div class="loading-container row-fluid width-limit">
    <div class="span2"> </div>
    <div class="span1"><img src="/twitter-bootstrap-theme/images/custom/spinner.gif" /></div>
    <div class="span9">Loading more updates...</div>
  </div>
</div>

  #if ($hasRole)
    <script type="text/javascript">
      $(document).ready(function() {
        $('.portlet select[multiple]:not(.ms-exempt)').multiselect({selectedList:1}).multiselectfilter();
      });
    </script>
  #end

<script src="$javascript_folder/production.min.js" type="text/javascript"></script>
<script type="text/javascript" src="//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-5494611e5b33a7e7"></script>
<!-- ra-521cfde96f062742 -->
$theme.include($body_bottom_include)

</body>

$theme.include($bottom_include)

</html>